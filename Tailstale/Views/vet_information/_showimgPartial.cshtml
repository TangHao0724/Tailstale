@model vet_information_ViewModel

@if (Model.URL == null)
{
    <img id="vet_photo" src="~/imgs/noimage.jpg" title="尚無照片" style="width:250px;height:250px" />
}
else
{
    <img id="vet_photo" src="~/lib/HospitalImages/Vet_Info/@Html.DisplayFor(model => model.URL)" title="@Html.DisplayFor(model => model.name)" style="width:250px;height:250px" />
}