@using Tailstale.Hospital_ViewModel
@model vet_information_ViewModel

@if (Model.URL == null)
{
    <img id="vet_photo" src="~/imgs/noimage.jpg" title="尚無照片" style="width:360px;height:360px" />
}
else
{
    <img id="vet_photo" src="~/lib/HospitalImages/@Html.DisplayFor(model => model.URL)" title="@Html.DisplayFor(model => model.name)" style="width:360px;height:360px" />
}